  <DataSets>
    <DataSet Name="AkcjePerMiasta">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KodTeryt">
            <Value>=Parameters!KodTeryt.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     YEAR(akc.DataRozpoczecia) AS Rok, CASE ter.IdTypuLok WHEN 5 THEN ter.Miasto WHEN 1 THEN ter.Nazwa ELSE 'DUPA' END AS Miasto, COUNT(1) 
                      AS [Liczba akcji], ter.KodTeryt
FROM         StrazPozarna.dbo.Akcje AS akc INNER JOIN
                      Teryt AS ter ON ter.idteryt = akc.IdTeryt
WHERE     (ter.KodTeryt LIKE @KodTeryt + '%')
GROUP BY YEAR(akc.DataRozpoczecia), ter.IdTypuLok, ter.Miasto, ter.Nazwa, ter.KodTeryt
ORDER BY Rok, [Liczba akcji] DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Rok">
          <DataField>Rok</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Miasto">
          <DataField>Miasto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Liczba_akcji">
          <DataField>Liczba akcji</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="KodTeryt">
          <DataField>KodTeryt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
