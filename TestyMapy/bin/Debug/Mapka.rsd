<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>DataSource1</DataSourceReference>
      <CommandText>select 
T.Nazwa
,KodTeryt
,Lokalizacja
	From Teryt as T
	Join TypyLokalizacji as lok on lok.IdTypuLok=T.IdTypuLok
	Where T.IdTypuLok=6</CommandText>
    </Query>
    <Fields>
      <Field Name="Nazwa">
        <DataField>Nazwa</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="KodTeryt">
        <DataField>KodTeryt</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Lokalizacja">
        <DataField>Lokalizacja</DataField>
        <rd:TypeName>Microsoft.SqlServer.Types.SqlGeography</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>