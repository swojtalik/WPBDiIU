  <DataSets>
    <DataSet Name="Gminy">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdPow">
            <Value>=Parameters!IdPow.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT T.Nazwa, T.KodTeryt, T.Lokalizacja
FROM     Teryt AS T INNER JOIN
                  TypyLokalizacji AS lok ON lok.IdTypuLok = T.IdTypuLok
WHERE  (T.IdTypuLok = 4) AND (T.KodTeryt LIKE @IdPow + '%')</CommandText>
      </Query>
