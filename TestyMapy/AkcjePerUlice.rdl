  <DataSets>
    <DataSet Name="AkcjePerUlice">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KodTeryt">
            <Value>=Parameters!KodTeryt.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	YEAR(akc.DataRozpoczecia) AS Rok
	,ter.Nazwa AS Ulica
	,COUNT(1) AS [Liczba akcji]
	,ter.KodTeryt
FROM
	StrazPozarna.dbo.Akcje AS akc
INNER JOIN
	[ZasobyWspolne].[dbo].Teryt AS ter
		ON	ter.idteryt = akc.IdTeryt
		AND	ter.IdTypuLok = 5
WHERE
	ter.KodTeryt LIKE @KodTeryt + '%'
GROUP BY YEAR(akc.DataRozpoczecia), ter.Nazwa, ter.KodTeryt
ORDER BY Rok, [Liczba akcji] DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Rok">
          <DataField>Rok</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Ulica">
